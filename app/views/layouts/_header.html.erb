<header>
	<%= link_to "logo", root_path %>
	<nav class="round">
		<ul>
			<li><%= link_to "Home", root_path %></li>
			<li><%= link_to "Help", help_path %></li>
			<% if user_signed_in? %> 
				<li>Signed in as <%= current_user.email %>. Not you? 
				<%= link_to 'Sign out', destroy_user_session_path, :method=>'delete' %></li>
			<% else %>
				<li><%= link_to "Sign up!", new_user_registration_path %> <%= link_to "Log in", new_user_session_path %></li>
			<% end %>
		</ul>
	</nav>
</header>